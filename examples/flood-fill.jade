extends _layouts/master

block context
  - var title = 'Flood Fill';

block scripts
  script(src='flood-fill.js')

block styles
  style.
    #threshold {
      position: absolute;
      top: 20px;
      right: 50px;
    }
    #threshold-value {
      color: white;
      position: absolute;
      top: 20px;
      right: 20px;
    }

block extra
  input(id="threshold", type="range", min="1", max="50", value="20")
  span(id="threshold-value")
